    "react-instantsearch-dom": "^6.11.0",
    "react-switch": "^6.0.0",
    "simple-icons": "^4.22.0",
    "styled-components": "^5.3.0",
    "swr": "^0.5.6",
    "use-sound": "^3.0.1"
