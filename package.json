  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "babel-eslint": "^10.1.0",
    "commitlint": "^12.1.1",
    "eslint": "^7.25.0",
    "eslint-config-react-app": "^6.0.0",
